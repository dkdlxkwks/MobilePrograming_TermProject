<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.androidtown.termproject">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Exercise"></activity>
        <activity android:name=".Note"></activity>
        <activity android:name=".Plan"></activity>
        <activity android:name=".Report"></activity>
        <activity android:name=".ExerciseTimer"></activity>
        <activity android:name=".ColorPaletteDialog"></activity>
        <activity android:name=".PenPaletteDialog"></activity>
        <activity android:name=".GallaryActivity"></activity>
        <activity android:name=".SplashActivity"></activity>
    </application>

</manifest>
